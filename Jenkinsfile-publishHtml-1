pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Test') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') {
       
        sh './mvn clean org.jacoco:jacoco-maven-plugin:prepare-agent install  '
        
        }
      }
    }
  }
}
